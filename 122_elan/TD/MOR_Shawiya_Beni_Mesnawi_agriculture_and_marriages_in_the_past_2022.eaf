        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>šḥal hadi? ah, hadi l-ʕaminat lli ma-ža-š ṣ-ṣayf, a had l-ʕaminat.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>w-kənna nḥăṛtu bḥal-kka ḍ-ḍṛa ka-tnfəʕ-na f-l-bhaym, w-ka-tnfəʕ-na fə-naklu-ha, w-dak ...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>l-ʕḏ̣ăm dyal-ha ka-nḥəmmiw bi-h, w-nəmšiw nḥăṭbu. w-nžibu l-xayrat, nžibu ... məlli ža ž-žafaf had-ši  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>ma-tlina-š qḍăṛ-na, ma-bqa-š [ma-bqa walu] ma-nadət-š ḍ-ḍṛa w-ma-tla-š ynuḍ l-ḥṭaw</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>wa-l-ḥamdu ḷḷah, l-ḥamdu ḷḷah, ʕla făḍḷu, lli ʕṭa-na ṛəbb-i măṛḥba bi-ha</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE/>
