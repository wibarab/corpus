        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>aywa ḥna ḍŭṛkaniya lləqṭu mən hŭk w-mən-hŭk-w-m... nqəḍiw l-ġaraḍ tkun ʕa ṣ-ṣăḥḥa w-s-salama [hadak ši lli kayn]</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>[daba šḥal mən sana w-nti sakna a xti ʕăyša hnaya?]</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>bədda džəwwəžt wəlla məlli zzətt? [ah] məlli zzətt? məlli zzətt ṛa ʕădd-i ši tmənya w-ṛəḅʕin wəllă təsʕud w-ṛəḅʕin</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>bḥal-ŭkkak wǝllă žayya l-xəmsin [tbark əllah. daba f-ʕŭmr-ək žayya l-xəmsin.] žayya l-xəmsin, talʕa l-xəmsin ma-bqa li-ya walu. [dzaditi hnaya zyada w-xluq wəllă tḥăwwəltu mən ši ḍŭwwaṛ axŭṛ?] lla lla zyada w-xluq a hiya xăymət-na hnaya w-ana džətt hnaya [aya] ana ʕădd-i hna ḍŭṛkaniya ṭalʕa f-ṛəḅʕa w-ţlaţin ʕam baš džəwwəžt.
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>[tbark ḷḷah b-ṭul l-ʕməṛ in ša ḷḷah, ḷḷah yəḥfəḍ mula ḍaṛ-ək w-yəḥfəḍ li-k l-wlĭdat, ya ṛăbb-i]</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>[guli lĭ-yya aš-mən qbila?] ḥna? [aya, mən...aš mən qbila?] ḥna wlad Məsnawi. [wlad Məsnawi ka-t-səmmaw] ah wlad Bu Ziri. [l-mən ka-t-... aš-mən mənṭaqa ka-trăžʕu lĭ-ha?]           
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>[wlad Məsnawi, mən aš mən mənṭaqa?] l-məntaqa ... wlad Bu Ziri, Xmis Sidi Mḥa bər Rḥal, iqlim ṣ-ṣṭat. [iqlim ṣ-ṣtat, yəʕni žihat š-šawiyya iyəh]  ah, žihat š-šawiya [had-ši lli bġina.]          </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>[iywa tqəḍṛi tʕawdi li-ya ʕla z-zwaž, šḥal hadi kifaš kan? w-daba ki-wəlla?] wa-ž-žwaž bəkri ma-kan-š əṛ-ṛăžəl yšuf lə-mra.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>ma-šənna-š ma-tšawfna-š, ma-tšawfna tta žina ʕərs ana, huwa ma-šaf-ni w-ana ma-štt-u ma-ʕṛăf-ni ki-dayra ana ḥămra wəlla huwa ṣfăṛ! iywa ... ka-yəmšiw yxaṭbu-k walĭdi-h.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>hadik s-saʕa tta ḥna ma-ka-ngulu walu, s-sif ʕli-k kətti baġa, hiya hadik, ma-kətti-š baġa, hiya hadik. l-muhim lli ka-ygul-ək džəwži ma-ddwi-š tti.
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>[aha b-s-sif ʕlik yəʕni ma-ʕənd-ək-š l-ḥaqq tguli ah wəlla lla.] ma-dguli-š ma-ʕădd-ək-š l-ḥaqq dguli ah wəlla lla.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>ga-ygulu lĭ-k džəwwži ṣafi ġadi džəwwži, ṣafi ġadya truḥi, ġadya truḥi, t-ši tbərki mʕa walĭdi-h, lli galu lik walĭdi hiya lli ddiri, ṣafi, ma-yəmkən-š ttfərʕəni wəlla t-guli ġadi nəmši hʷukka wəlla hʷukka, lla ma-kayn-š, ṣafi tə-bqay təḥt s-săyṭaṛa dyal walĭdi-h</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>təbqay ... hadi hiya, had-ši lli kan, drŭka tbəddəl l-wəqt, ḍṛŭka qabla ma-qabla-š wăxxa təmši gaʕ txŭṭbi wəlla ma-ʕrăti-š aš ddiri, lla kant hiya ma-qabla-š, ma-qabla-š, lə-ʕris tta huwa lla galu lĭ-h walĭdi-h ṣafi "ġa-taxŭd hadik", ṣafi yəmkən lĭ-h yaxŭd hadik.                 </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>ḍṛŭka lla galu l-a... ma-bġa-š, ma-bġa-š xa ykun ma-ʕərt aš dayər, d-dənya tbəddlət bəzzaf! [bəzzaf.] iwa, dik əs-saʕa ma-kan-š bḥal-ŭk, kan ʕa lli žab ma-kanu-š xwatəm, ma-kanu-š had-ši dyal l-ažwaq, ma-kanu-š ...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>kanu ʕa lli dda, a lli dda ṣafi [a lli dda zəʕma.] ʕa lli dda fə-l-ḥwayž, ʕa lli dda fi-hada, ʕa lli dda, ma-kan-š had-ši fəṣṣaḷ xăyyaṭ, lə-ksawi dak-ši bḥal-kkaniya miya zəʕma w-la bədda mən təkšiṭa wəlla bədda mən hadi, ʕa lli kan, ʕa lli kan dak-ši qlil ṣafi, ka-ygulu ḷḷah yĭssər, ma-kan-š kaġṭ, ma-kan-š kaġṭ, ana tta wlətt wəlidati b-s-salama kamlin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
