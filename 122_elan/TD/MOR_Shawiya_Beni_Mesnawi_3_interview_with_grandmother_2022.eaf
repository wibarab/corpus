        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>...wəlli b-tayt ʕli-ha wəlli ngul l-ək [xši liʕat.] lla. ngul l-ək tbina, tbina. ha tti žăyba li-k əl-băyḍ əl-bəldi. dyal l-ʕrubiya, dyal hadik l-džaža, lli dawra ṛa-ha fina hiya ... </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>w-ʕṭăyt li-hum, ʕṭăyt əl-năʕža, w-ʕṭăyt l-... ḥaša-k, l-ḥmiṛat.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>w-ʕṭăyt əl-bəgirat. w-brəkt xăržt ndir ḥətta haw ta-təllgət ʕli-ya d-dərriya. gatt kti "fin tilifun-ək?" gŭlt li-ha: "a wili ya ṃṃʷi ṛa ana məxliya f-əl-xăyma-w ...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>w-barka xarž, ʕaddat xəržt nəžri, w-ḷḷah qəmišt-i ma-bəddəlt-ha-š haga?? bi-ha t-tbən, bi-ha kull-ši, w-ṣəxx w-kull-ši </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>[zwina ma ša ḷḷah, zina tbark ḷḷah.]</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>hada huwa ḥal əd-dŭnya, a ṃṃʷi, huwa hada, lli bġăyti-ni, guli li-ya, ngul-u li-k, [nsul l-ək, a l-hažža, daba ntiyya ...]</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>[tbark ḷḷah, ḷḷah yəṭwəl f-ʕəmr-ək, guli li-ya ki ka-tžik had əl-wŭqt w-kifaš tbəddlət ʕla l-wŭqt lli ʕəšti fi-ha faš kətti ṣġira w-mazal ṣġira w-mazala ʕad tzəwwəžti w-dərti wlidat-ək?]           </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>wa-ṃṃʷi, aš ngul l-ik, ana ma-nʕṛăf-š ndwi, w-bəkri kənna măḥnin, kənna nḥəlbu, kan lə-bgăṛ, kanu ž-žmal.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>kanu l-walĭdin, kanu l-mṭaməṛ taʕ ẓ-ẓṛăʕ, fhəmti? [iyəh l-mṭaməṛ.] l-mṭaməṛ ka-yḥăfṛu mən kʷbarin, tăḥt l-aṛḍ [w-yəxšiw fi-hum ẓ-ẓṛăʕ.] iyəh yəxšiw fi-hum ẓ-ẓṛăʕ w-yməllsu ʕli-h, wŭqt ma-bġăyti tžəbdi tḥin ṭəḥni-h ṛa-k tti, lli ʕaṛəfa [...] tžəbdi nəṣṣ xănša, tžəbdi ʕăbərtin, tžəbdi ...                             </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>w-haw tnəggi-ha w-ta-ddi-ha l ṭ-ṭaḥuna w-nṭḥănu-hu, a ṃṃʷi, w-ndiru l-məll, kan bəkri ʕa l-məll [l-məll, ašna-hu?] l-məll, ka-tšəddi ṛ-ṛḍad, diri-h fug əl-mnayṣbat ta-yḥma, bəʕd yəḥma diri waḥəd ... diri ḥaš-ăk [ʕəzz-ək aḷḷah] diri waḥəd-əl-qăṣdira, dayra bḥal-kayk, ka-ṣ-ṣiniya.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>tḥăṭṭi-ha w-təṛmi fi-ha l-məlla wə-tkfi ʕli-ha ṛ-ṛḍaḍ, w-ṭăgbi-ha, diri ʕa-l-gəšgəš mən fug. l-ḥṭăb, l-bal, l-bal ndiru ḥna l-bal, ʕa-l-bal tḥăṭbi-hu</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>w-ta-zigdi l-ʕafya mən fug ṛ-ṛḍad kayək, w-hiya b-tži tgəlbi ʕli-ha bḥal kəka b-əz-zəllag ḥaša-k tṣĭbi-ha ḥmăṛt həkkak ...           
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>ḍṛuk, a ṃṃʷi, ʕad əl-krun, ḍṛuka ʕad əl-krun bḥal ʕaiša, bənt-i, kaykak ... bḥal ʕad əl-fəṛṛan, dukat ʕada bu qafla ʕad əl-fəṛṛan </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>w-a ṃṃʷi had-l... kŭnna ntfəllḥu, kənna nḥăṛtu b-əž-žmal [ntuma ʕyalat kəttu ka-ddiru had-ši?] lla, yḥăṛtu ġir r-ržala w-ḥna, nddiw li-hum l-ġda, w-nddiw li-hum l-makla w-yḥăṣḍu b-əl-mənžəl bḥal kika [###] b-l-ġʷmara, kay-hŭk. 
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>... [ma-kan dik s-saʕa walu, lla trakturat, lla walu] walu, walu yḥăṣḍu ʕa b-əž-žmal w-b-əl-bhaym.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>w-ṃṃʷi, ydərsu b-əl-bhaym, b-əl-ṛ-ṛwa yṛəgwiw w-yrăbṭu mən hna, mən hna, wəḥda ʕla wəḥda, w-yḍuṛ ər-rəgway w-yrəgwi-h ki-həkkaya.        </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>w-ydərriw bʷ-l-mədra bḥal kayka (... ḥəṣid l-mʷakən) ḍṛuka t-tbən yhăwwəd l-žiha w-ẓ-ẓṛăʕ yəbqa fi-žiha, aywa nwəggfu-h kəykaya w-nžibu li-h l-ʕăbṛa, w-nʕăbbṛu-h w-ndiru-h f-əl-xayṭ w-ndexxnu w-ndiru-h f-əl-mṭameṛ.                      </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>ḍṛuk, a ṃṃʷi, ʕad əl-makina w-ʕad traktur yəḥṛăt w-l-makina təḥṣăd w-had, lli năʕṛəf, ya ṃṃʷ-i, huma hada, ya ḥnint-i.                              </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>... ḍṛuk had əl-wŭqt xuṛ əl-bniya ʕad wŭqt xuṛ ḍṛuka, imma l-ḥamdu ḷḷah, bəkri kənna zəʕma, kənna nṭăḥnu f-əṛ-ṛḥa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>ṭ-ṭḥin ila ḅṭa ʕli-na ṭ-ṭaḥuna wəlla šăy nnəggiw l-gəmḥ w-nstiw-u nnəggiw-u f-əṣ-ṣăyniya [iyəh] aywa ntəfflu ṛ-ṛḥa, ṛa-h ʕad ʕaiša, bənt-i, ila bġiti tṣəwwəṛi-ha [daba in ša aḷḷah nṣəwwəṛ-ha] kbira, ḥna ʕădd-na ṛ-ṛḥa kbira taʕt l-walĭdin, mḥăqqa taʕt žirana ḍṛuk.              
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>aywa, ntəfflu ndiru waḥəd-ṭ-ṭḥan bḥal xədma hăkka, ndiru-h bḥal hŭk, nšru-h w-nḥăṭṭu ... žibi li-ha tay ... w-nḥăṭṭu ṛ-ṛḥa hna, w-ṛṛəmiw b-šwiya b-šwiya, ntmăḥnu maš ### [iyəh b-ṭulət ʕəmṛ-ək, ḷḷah yəʕṭək ṣḥăyḥt-ək] w-nṭăḥnu nṭăḥnu, ta-ndiru ḥsab baš nfăwgŭṛu.                                 </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>nfăwgŭṛu b-... qăṣriya nʕămmṛu kăyka kəskəsu w-nkəskəsu-h w-nzərdu, nsəggmu-h w-nfăwgŭṛu-h w-naklu-h</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>kant ʕa bṣăyḷa, ṃṃʷ-i, w-xʷuizzugwat, w-l-ləftat, maši bḥal had əl-žil, lli ʕad ḍṛuk ... ʕa xʷuizzuwat ʕa ka-tqăṛṛəb ṭqis w-l-lfətat    
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>aywa ha, lli năʕṛəf, a ṃṃʷi, huwa hada ḥənna, wa had-ši, lli nʕăṛfu bəkri.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>wa-ntfəllḥu zəʕma w-yži nḥăṛtu l-ful, kanu yġərsu l-băṣḷa</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
