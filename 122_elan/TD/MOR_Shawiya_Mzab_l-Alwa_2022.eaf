        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>tbəddəl, baš tbəddəl, tbəddel l-ašġal, tbəddəl šwiya, wəllat əl-qṛaya</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>bəkri ma-kan-š ən-nas, ma-qaṛiya-š, ma-ʕaṛfa-š. n-nas bəkri baš kant xəddama? </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>baš xəddamin b-ən-niya ... n-niya. n-niya, hiya, lli kant əl-barəḥ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>waš l-ʕṛas dyal daba wəlla dyal bəkri? ### hna kanu ʕădd-na l-ʕṛas. kan l-insan, məlli kun baġi ydir əl-ʕăṛs. ka-y ... kan əl-qbila kamla ka-tfṛăḥ šhăṛ w-hiya təfṛăḥ. l-yum ma-bqaw-š n-nas yfăṛḥu wəlla ka-ydiru-hum f-əl-qaʕat wəlla ḥṣăb xŭṛ.  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>kan əl-insan məlli ġadi ykun ši ʕăṛs f-əl-qbila n-nas ka-tfṛăḥ. ka-tfṛăḥ, ka-tfṛăḥ ʕadad w-uma fəṛḥanin. šhăṛ wəlla šhăṛ w-nəṣṣ wəlla šăhrăyn, l-yum lla, ma-bqa-š ak-ši ... məlli ykun l-ʕăṛs, n-nas ka-tfṛăḥ, ka-nštăḥ, ka-tfəṛṛəž.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>ma-kayn-š xṣuma, ma-kayn-š hadi, ma-kayn-š dak-ši, ka-ybqa xxa, ma-kant lla kamera, lla walu, kan našaṭ ... kan našaṭ ʕla daba. w-hadak ši taʕ bəkri ma-mʕawŭḍ-š l daba. dak-ši taʕ ... dyal ... w-hna ... dyal bəkri ma-huwa-š huwa hada. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>bəkri kanu l-ġŭnna, daba ma-bqa-š əl-ġŭnna. [škun kan ka-yġănni, a š-šrif, sməḥ li-ya qăṭʕat-ək, f-əl-ʕṛasat?] l-awwlin, lli mʕaya na ... š-šʕăbi, Qaṛzuz w-Măḥraš [kant əl-ʕăyṭa?] ay, huwa, lli mxərrəž l-ʕăyṭa dyal l-ʕălwa, Qaṛzuz w-Măḥraš, huma, lli mxərržin, mxərržin l-ʕăyṭa dyal l-ʕălwa. kayna z-zăḥḥafa f-sṭaṭ matat, ḷḷa yərḥəm-ha. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>mŭġănniya Zawiya "ma-na saxi bi-k" tta hiya mxărrəža-ha məṛ ṛ-ṛaṣ dyal-ha dămġa-ha məṛ ṛ-ṛaṣ dyal-ha ... maši ʕad qălldat-ha mən žiha ŭxṛa, lla. [dyal-ha?] ah, ġənnat-ha hiya. kanu ʕădd-na hna bəkri na wa ... mŭġănni, ka-yġănni məṛ ṛaṣ-u, maši yqălləd wəlla ka-ysmăʕ, lla! lla, ma-ntsăntu-š li-h hadak, ma-ʕădd-na-š, ma-ndiru ... smăʕdi, la ma-kan-š mġănni-a mər ṛaṣ-u ma-ntsăntu-š, ma-nšriw-š disk dyal-u, w-ntsăntu li-h, lla.                       </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>lla, ḥənt ʕa mqălləd, wanni dyal-u kan ntsăntu li-h. kan Qaṛzuz w-Măḥraš, l-uwlin mxărržin l-ʕăyṭa dyal l-ʕălwa, kayna zăḥḥafa f-əṣ-ṣṭaṭ mxărrža, dak kəyn Bu šʕăyb Băyḍawi mxărrž əl-Măṛṣawi.          </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a51"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>kan ši dyal bəkri, kan zin, maši bḥal dyal daba.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a52"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>wəllat əl-qaʕa wəlla ... l-ḅaf wəlla hada wəlla hada dak-ši ... nqăṣ. [kanu l-ʕṛăsət ka-ydaru f-əz-zənqa bḥal hakkak, gyaṭən ... ] maši f-əz-zănqa, f-əḍ-ḍaṛ ka-ddiru xuzăyn, dyal l-xyam.  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a54"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>[šḥal kant əl-mŭdda dyal l-ʕăṛs? šḥal ka-ddiru mən əl-yum? ...] aaah. mŭdda dyal ... kayn l-xtama, nhaṛ w-lil.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a57"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>kayn əl-ʕris, ykun mrăwaḥ əl-ʕṛuṣ, ka-ḍḍṛŭḅ tta l sbăʕ iyyam. ka-ydŭwwzu mʕa-h l-lila dyal-u w-l-ġădd ka-yəmšiw ka-yḍuṛu ... f-əl-qăbail w-f-əl-qbila dyal-hum, ka-yḍuṛu š-kən gulu li-hum ḥna ka-ngulu ka-yṣăṛṛfu.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a60"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>ta-yžəmʕu l-flus, yžəmʕu l-băyḍ, w-yzidu fi-h. l-ġădd tani ybšiw l-žiha ŭxṛa, l-ġădd tan ka-dduz sbăʕ iyyam, sətt iyyam. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a61"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>ka-tṣăṛṛfu, ka-ngulu hna ka-yṣăṛṛfu. [ka-yṣəṛṛfu, waš ka-yžəmʕu wəlla yăʕṭiw ] ka-yžəmʕu, ka-yžəmʕu [zəʕma n-nas dyal l-qbila ka-ysăhmu mʕa-hum ...] ka-ngulu ka-yṣăṛṛfu.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a69"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>kan əl-ʕris ma-ka-yšuf-š əl-ʕṛuṣ, ma-yšuf-ha-š ʕămbṛ-u ma-šaf-ha, ka-yxăṭbu-ha li-h walidi-ha, walidi-h. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a70"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>ṃṃʷ-u w-ḅḅʷa-h, lli ka-yəbšiw li-ha. huwa ma-yšuf-ha tta ka-tži ʕădd-u hadik əl-lil. ʕad ka-yšuf-ha.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a72"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>[w-lli kan yəṭlăqaw ḥda l-bir.] xălli haduk ḥda l-bir. hadik bənt əḍ-ḍŭwwaṛ. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a73"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>ana dwit l-ək ʕla mṛa bəṛṛaniya. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a74"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>ana wa ... ḥit walidin-na ka-ygu-k "ḥăṛṭa qriba w-mṛa ġriba". </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a76"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>mṛa məzyan taxŭd-ha bʕida.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a78"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>huma ... daba ki, lli ma-kan-š yšuf-ha gaʕ tta tži ʕădd-u hadik əl-lil.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a79"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>w-dayrin waḥəd-əl-ʕada ... məlli ka-yṛŭbḥu, ka-yžibu ʕli-ha hiya hazza fərda dyal l-bəlġa dyal-ha, dyal š-šərbil dyal-ha.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a82"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>huwa hazz fərda, məlli ka-yḥŭll ʕli-ha l-ġŭrfa dyal-ha, l-măḥăll dyal-ha, imma sbəqt-u w-hiya ḍṛəbt-u, ġada dži ġalba-h, səbqa huwa, ġadi ykun ġaləb. had-ši, lli kanu dayrin f-əl-ʕadat dyal bəkri. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a94"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>bḥal dak-ši ... w-l-mṛa ka-tbši tani nhaṛ li-h, nhaṛ tani ... la ḍăṛḅt-u, hiya dyal l-uwwla ka-txŭrž ġalba-h, ʕa ka-ygul ... w-dima ka-ybqa ma-ydir ši-ḥaža tta yšăwwər-ha. [...] ana? lla ġləbt .. ana ... baqa, daba mart-i, mart-i daba na ...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
