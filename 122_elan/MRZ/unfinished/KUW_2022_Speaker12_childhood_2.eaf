            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>The people started to come to the cities</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>And before this, where did they live?</ANNOTATION_VALUE>
