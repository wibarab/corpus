        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>Do you remember this?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>No, I mean when did the people start to become sedentarized, meaning in which years, which years did they start to gather in the cities</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
